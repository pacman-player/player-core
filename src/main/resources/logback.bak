<?xml version="1.0" encoding="UTF-8"?>
<configuration version="1.0" name="LoggingConf">

    <appender name="Console-Appender" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} %highlight(%-5level) %magenta([%logger{30}.%M]) - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="AdminAuthorRestController" level="info" additivity="true">

        <appender name="FileInfo" class="ch.qos.logback.core.FileAppender">

            <file>Logs/AdminAuthorRestController/info.log</file>

            <encoder>
                <pattern>%d{YY/MM/DD HH:mm:ss} %-5level %logger{40}.%M - %msg%n</pattern>
            </encoder>

            <filter class="ch.qos.logback.classic.filter.LevelFilter">
                <onMismatch>DENY</onMismatch>
                <onMatch>ACCEPT</onMatch>
                <level>ERROR</level>
            </filter>

        </appender>

        <appender name="FileWarn" class="ch.qos.logback.core.FileAppender">
            <file>Logs/AdminAuthorRestController/warn.log</file>
            <encoder>
                <pattern>%d{YY/MM/DD HH:mm:ss} %-5level %logger{40}.%M - %msg%n</pattern>
            </encoder>
            <filter class="ch.qos.logback.classic.filter.LevelFilter">
                <onMismatch>DENY</onMismatch>
                <onMatch>ACCEPT</onMatch>
                <level>ERROR</level>
            </filter>
        </appender>

        <appender name="FileError" class="ch.qos.logback.core.FileAppender">
            <file>Logs/AdminAuthorRestController/error.log</file>
            <encoder>
                <pattern>%d{YY/MM/DD HH:mm:ss} %-5level %logger{40}.%M - %msg%n</pattern>
            </encoder>
            <filter class="ch.qos.logback.classic.filter.LevelFilter">
                <onMismatch>DENY</onMismatch>
                <onMatch>ACCEPT</onMatch>
                <level>ERROR</level>
            </filter>
        </appender>

        <appender-ref ref="Console-Appender"/>
    </logger>

